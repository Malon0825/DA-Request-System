<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SignupScreen">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />


    <ImageView
        android:id="@+id/imageView84"
        android:layout_width="352dp"
        android:layout_height="701dp"
        android:background="@drawable/login_window_bg"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="302dp"
        android:layout_height="79dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView84"
        app:layout_constraintEnd_toEndOf="@+id/imageView84"
        app:layout_constraintStart_toStartOf="@+id/imageView84"
        app:layout_constraintTop_toTopOf="@+id/imageView84"
        app:layout_constraintVertical_bias="0.067"
        app:srcCompat="@drawable/tb_fname" />

    <ImageView
        android:id="@+id/imageView94"
        android:layout_width="302dp"
        android:layout_height="79dp"

        app:layout_constraintBottom_toBottomOf="@+id/imageView84"
        app:layout_constraintEnd_toEndOf="@+id/imageView101"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView101"
        app:layout_constraintTop_toBottomOf="@+id/imageView101"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/tb_password" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="302dp"
        android:layout_height="79dp"

        app:layout_constraintBottom_toBottomOf="@+id/imageView84"
        app:layout_constraintEnd_toEndOf="@+id/imageView94"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView94"
        app:layout_constraintTop_toBottomOf="@+id/imageView94"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/tb_con_password" />

    <ImageView
        android:id="@+id/imageView101"
        android:layout_width="302dp"
        android:layout_height="79dp"

        app:layout_constraintBottom_toBottomOf="@+id/imageView84"
        app:layout_constraintEnd_toEndOf="@+id/imageView93"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView93"
        app:layout_constraintTop_toBottomOf="@+id/imageView93"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/tb_address" />

    <ImageView
        android:id="@+id/imageView93"
        android:layout_width="302dp"
        android:layout_height="79dp"

        app:layout_constraintBottom_toBottomOf="@+id/imageView84"
        app:layout_constraintEnd_toEndOf="@+id/imageView92"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView92"
        app:layout_constraintTop_toBottomOf="@+id/imageView92"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/tb_email" />

    <ImageView
        android:id="@+id/imageView92"
        android:layout_width="302dp"
        android:layout_height="79dp"

        app:layout_constraintBottom_toBottomOf="@+id/imageView84"
        app:layout_constraintEnd_toEndOf="@+id/imageView91"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView91"
        app:layout_constraintTop_toBottomOf="@+id/imageView91"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/tb_lname" />

    <ImageView
        android:id="@+id/imageView91"
        android:layout_width="302dp"
        android:layout_height="79dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView84"
        app:layout_constraintEnd_toEndOf="@+id/imageView9"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView9"
        app:layout_constraintTop_toBottomOf="@+id/imageView9"
        app:layout_constraintVertical_bias="0.003"
        app:srcCompat="@drawable/tb_mname" />

    <EditText
        android:id="@+id/txtFName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:theme="@style/ThemeOverlay.ListView"
        app:layout_constraintBottom_toBottomOf="@+id/imageView9"
        app:layout_constraintEnd_toEndOf="@+id/imageView9"
        app:layout_constraintStart_toStartOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        app:layout_constraintVertical_bias="0.411" />

    <EditText
        android:id="@+id/txtMName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:theme="@style/ThemeOverlay.ListView"
        app:layout_constraintBottom_toBottomOf="@+id/imageView91"
        app:layout_constraintEnd_toEndOf="@+id/imageView91"
        app:layout_constraintStart_toStartOf="@+id/imageView91"
        app:layout_constraintTop_toBottomOf="@+id/imageView9" />

    <EditText
        android:id="@+id/txtLName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:theme="@style/ThemeOverlay.ListView"
        app:layout_constraintBottom_toBottomOf="@+id/imageView92"
        app:layout_constraintEnd_toEndOf="@+id/imageView92"
        app:layout_constraintStart_toStartOf="@+id/imageView92"
        app:layout_constraintTop_toTopOf="@+id/imageView92" />

    <EditText
        android:id="@+id/txtEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:theme="@style/ThemeOverlay.ListView"
        app:layout_constraintBottom_toBottomOf="@+id/imageView93"
        app:layout_constraintEnd_toEndOf="@+id/imageView93"
        app:layout_constraintStart_toStartOf="@+id/imageView93"
        app:layout_constraintTop_toTopOf="@+id/imageView93" />

    <EditText
        android:id="@+id/txtAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:theme="@style/ThemeOverlay.ListView"
        app:layout_constraintBottom_toBottomOf="@+id/imageView101"
        app:layout_constraintEnd_toEndOf="@+id/imageView101"
        app:layout_constraintStart_toStartOf="@+id/imageView101"
        app:layout_constraintTop_toTopOf="@+id/imageView101" />

    <EditText
        android:id="@+id/textPassword"
        android:layout_width="181dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPassword"
        android:theme="@style/ThemeOverlay.ListView"
        app:layout_constraintBottom_toBottomOf="@+id/imageView94"
        app:layout_constraintEnd_toEndOf="@+id/imageView94"
        app:layout_constraintHorizontal_bias="0.388"
        app:layout_constraintStart_toStartOf="@+id/imageView94"
        app:layout_constraintTop_toTopOf="@+id/imageView94"
        app:layout_constraintVertical_bias="0.476" />


    <EditText
        android:id="@+id/txtConfPassword"
        android:layout_width="181dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPassword"
        android:theme="@style/ThemeOverlay.ListView"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.408"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toBottomOf="@+id/imageView94"
        app:layout_constraintVertical_bias="0.416" />

    <ImageButton
        android:id="@+id/btn_signup"
        android:layout_width="202dp"
        android:layout_height="65dp"
        android:background="@drawable/btn_signup"
        app:layout_constraintBottom_toBottomOf="@+id/imageView84"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toBottomOf="@+id/imageView11"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/btn_goback"
        android:layout_width="65dp"
        android:layout_height="63dp"
        android:background="@drawable/btn_go_back"
        app:layout_constraintBottom_toBottomOf="@+id/btn_signup"
        app:layout_constraintEnd_toStartOf="@+id/btn_signup"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imageView84"
        app:layout_constraintTop_toTopOf="@+id/btn_signup"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/btnVisibility"
        android:layout_width="41dp"
        android:layout_height="42dp"
        android:background="@drawable/hide"
        app:layout_constraintBottom_toBottomOf="@+id/textPassword"
        app:layout_constraintEnd_toEndOf="@+id/imageView94"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textPassword"
        app:layout_constraintTop_toTopOf="@+id/textPassword"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/btnConfVisibility"
        android:layout_width="41dp"
        android:layout_height="42dp"
        android:background="@drawable/hide"
        app:layout_constraintBottom_toBottomOf="@+id/txtConfPassword"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/txtConfPassword"
        app:layout_constraintTop_toTopOf="@+id/txtConfPassword"
        app:layout_constraintVertical_bias="0.0" />


</androidx.constraintlayout.widget.ConstraintLayout>